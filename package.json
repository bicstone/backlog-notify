{
  "name": "backlog-notify",
  "version": "1.0.1",
  "description": "Notify commit messages to Backlog issue",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn lint:fix && ncc build ./src/main.ts",
    "lint": "eslint --ext .ts ./src/ --max-warnings 0",
    "lint:fix": "eslint --fix --ext .ts ./src/ --max-warnings 0",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicstone/backlog-notify/backlog-notify.git"
  },
  "keywords": [
    "Backlog",
    "Notify",
    "Project management"
  ],
  "author": "Takanori Oishi",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash.template": "4.5.0",
    "@types/node": "^12",
    "@vercel/ncc": "0.29.2",
    "eslint": "7.32.0",
    "husky": "7.0.1",
    "lodash.template": "4.5.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "ts-standard": "^10.0.0"
  }
}
